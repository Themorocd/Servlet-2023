<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletInicio</servlet-name>
        <servlet-class>Controlador.ServletInicio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletAlta</servlet-name>
        <servlet-class>Controlador.ServletAlta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletBorrar</servlet-name>
        <servlet-class>Controlador.ServletBorrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>modificar</servlet-name>
        <servlet-class>Controlador.modificar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletModificar</servlet-name>
        <servlet-class>Controlador.ServletModificar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletInicio</servlet-name>
        <url-pattern>/ServletInicio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletAlta</servlet-name>
        <url-pattern>/ServletAlta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletBorrar</servlet-name>
        <url-pattern>/ServletBorrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>modificar</servlet-name>
        <url-pattern>/modificar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletModificar</servlet-name>
        <url-pattern>/ServletModificar</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
