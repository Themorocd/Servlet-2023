        <servlet-name>ServletBorrar</servlet-name>
        <url-pattern>/ServletBorrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servletbuscotitulo</servlet-name>
        <url-pattern>/Servletbuscotitulo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servletbuscopelipordirector</servlet-name>
        <url-pattern>/Servletbuscopelipordirector</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
