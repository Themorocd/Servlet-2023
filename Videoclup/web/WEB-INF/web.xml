    <servlet-mapping>
        <servlet-name>ServletInicio</servlet-name>
        <url-pattern>/ServletInicio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servletalta</servlet-name>
        <url-pattern>/Servletalta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServleModificar</servlet-name>
        <url-pattern>/ServleModificar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlemodifinal</servlet-name>
        <url-pattern>/Servlemodifinal</url-pattern>
    </servlet-mapping>
