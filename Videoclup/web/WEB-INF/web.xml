    </servlet>
    <servlet>
        <servlet-name>Servlemodifinal</servlet-name>
        <servlet-class>Controlador.Servlemodifinal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletBorrar</servlet-name>
        <servlet-class>Controlador.ServletBorrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servletbuscotitulo</servlet-name>
        <servlet-class>Controlador.Servletbuscotitulo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servletbuscopelipordirector</servlet-name>
        <servlet-class>Controlador.Servletbuscopelipordirector</servlet-class>
